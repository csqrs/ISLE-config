CREATE DATABASE DPDB CHARACTER SET utf8 COLLATE utf8_general_ci;
CREATE USER 'DPDBU'@'%' IDENTIFIED BY 'DPDBPW';
GRANT SELECT, INSERT, UPDATE, DELETE, CREATE, DROP, INDEX, ALTER, ALTER ROUTINE, CREATE ROUTINE, CREATE TEMPORARY TABLES, CREATE VIEW, EVENT, EXECUTE, LOCK TABLES, REFERENCES, SHOW VIEW, TRIGGER ON DPDB.* TO 'DPDBU'@'%';
FLUSH PRIVILEGES;
